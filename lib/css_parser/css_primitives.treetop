grammar CSSPrimitives
  include CSSString
  include CSSNumber
  include CSSFrequency
  include CSSPercentage
  include CSSTime
  include CSSAngle
  include CSSColor
  include CSSLength
  include URL
  include CSSSymbols

  rule data_primitive
    length / percentage / color / space / url / angle / time / frequency
  end
end