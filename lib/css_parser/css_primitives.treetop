grammar CSSPrimitives
  include CSSFrequency
  include CSSPercentage
  include CSSTime
  include CSSAngle
  include CSSColor
  include CSSLength
  include URL
  include CSSSymbols

  rule primitive
    length / percentage / value / space / url / angle / time / frequency
  end

  rule value
    color / "10px"
  end

  rule number
    sign? positive_number
  end

  rule positive_number
    [0-9]+
  end

  rule float
    number ("." number)? 
  end

  rule string
    [a-zA-Z\.\/]+
  end
end