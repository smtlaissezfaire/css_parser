grammar CSSPrimitives
  include CSSSymbols

  include CSSNumber
  include CSSLength
  include CSSPercentage
  include URL
  include CSSColor
  include CSSAngle
  include CSSTime
  include CSSFrequency
  include CSSString

  rule data_primitive
    length / percentage / color / space / url / angle / time / frequency
  end
end